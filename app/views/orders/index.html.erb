<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Amount</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.user_id %></td>
        <td><%= order.amount %></td>
        <td><%= order.created_at %></td>
        <td><%= order.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
